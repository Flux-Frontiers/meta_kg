  <!-- Reactions -->
  <!-- Alanine aminotransferase: L-Alanine + 2-OG ↔ Pyruvate + L-Glutamate -->
  <reaction id="51" name="rn:R00258" type="reversible">
    <substrate name="cpd:C00041"/>
    <substrate name="cpd:C00026"/>
    <product name="cpd:C00022"/>
    <product name="cpd:C00025"/>
  </reaction>

  <!-- Aspartate aminotransferase: L-Aspartate + 2-OG ↔ Oxaloacetate + L-Glutamate -->
  <reaction id="52" name="rn:R00355" type="reversible">
    <substrate name="cpd:C00049"/>
    <substrate name="cpd:C00026"/>
    <product name="cpd:C00036"/>
    <product name="cpd:C00025"/>
  </reaction>

  <!-- Glutamate dehydrogenase: L-Glutamate + NAD+ + H2O → 2-OG + NH3 + NADH -->
  <reaction id="53" name="rn:R00248" type="reversible">
    <substrate name="cpd:C00025"/>
    <substrate name="cpd:C00003"/>
    <substrate name="cpd:C00001"/>
    <product name="cpd:C00026"/>
    <product name="cpd:C00014"/>
    <product name="cpd:C00004"/>
  </reaction>

  <!-- Glutamate dehydrogenase (NADP): L-Glutamate + NADP+ + H2O → 2-OG + NH3 + NADPH -->
  <reaction id="54" name="rn:R00243" type="reversible">
    <substrate name="cpd:C00025"/>
    <substrate name="cpd:C00006"/>
    <substrate name="cpd:C00001"/>
    <product name="cpd:C00026"/>
    <product name="cpd:C00014"/>
    <product name="cpd:C00005"/>
  </reaction>

  <!-- Glutamine synthetase: L-Glutamate + NH3 + ATP → L-Glutamine + ADP + Pi -->
  <reaction id="55" name="rn:R00253" type="irreversible">
    <substrate name="cpd:C00025"/>
    <substrate name="cpd:C00014"/>
    <substrate name="cpd:C00002"/>
    <product name="cpd:C00064"/>
    <product name="cpd:C00008"/>
    <product name="cpd:C00009"/>
  </reaction>

  <!-- Glutaminase: L-Glutamine + H2O → L-Glutamate + NH3 -->
  <reaction id="56" name="rn:R00256" type="irreversible">
    <substrate name="cpd:C00064"/>
    <substrate name="cpd:C00001"/>
    <product name="cpd:C00025"/>
    <product name="cpd:C00014"/>
  </reaction>

